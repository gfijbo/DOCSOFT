{# app/Resources/views/traitement.html.twig #}
{# Onglets pour TRAITEMENTS #}
{% block li4 %}
{% if listOnglets is defined %}
{% for onglet in listOnglets %}
<li class="nav-item" data-toggle="tooltip" data-placement="right" title="Menu Levels">
	<a class="nav-link nav-link-collapse collapsed" data-toggle="collapse"
	href="#collapseMulti{{ loop.index }}" data-parent="#exampleAccordion">
		<i class="fa fa-fw fa fa-cog"></i>
		<span class="nav-link-text">{{ onglet.getOnglet() }}</span>
	</a>
	<ul class="sidenav-second-level collapse" id="collapseMulti{{ loop.index }}">
	{% for page in onglet.getPages() %}
		<li><a href="{{ page.getUrl() }}">{{ page.getPage()}}</a>
		</li>
	{% endfor %}
	</ul>
{% endfor %}
{% endif %}
{% endblock %}
